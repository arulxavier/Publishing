<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fixent.publish.server.model.Address" table="ADDRESS">
	
		<id name="id" type="java.lang.Integer">
			<column name="ID" precision="5" scale="0" />
			<generator class="increment" />
		</id>

		<property name="street1" type="string">
			<column name="STREET_1" not-null="true" />
		</property>
		<property name="street2" type="string">
			<column name="STREET_2" not-null="true" />
		</property>
		<property name="city" type="string">
			<column name="CITY" not-null="true" />
		</property>
		<property name="state" type="string">
			<column name="STATE" not-null="true" />
		</property>
		<property name="country" type="string">
			<column name="COUNTRY" not-null="true" />
		</property>
		<property name="pincode" type="java.lang.Integer">
			<column name="PINCODE" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>
