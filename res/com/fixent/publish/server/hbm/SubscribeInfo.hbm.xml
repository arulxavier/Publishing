<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fixent.publish.server.model.SubscribeInfo" table="SUBSCRIBE_INFO">
		<id name="id" type="java.lang.Integer">
			<column name="ID"/>
			<generator class="increment" />
		</id>
		
		<property name="noOfYear" type="java.lang.Integer">
			<column name="NO_OF_YEAR" length="20" not-null="true" />
		</property>
		<property name="subscribeDate" type="date">
			<column name="START_DATE" length="20" not-null="true" />
		</property>		
		<property name="expiredDate" type="date">
			<column name="END_DATE" not-null="true" />
		</property>
		
		 <many-to-one name="book" class="com.fixent.publish.server.model.Book" fetch="select">
            <column name="BOOK_ID" not-null="true" />
        </many-to-one>
        
         <many-to-one name="subscriber" class="com.fixent.publish.server.model.Subscriber" fetch="select">
            <column name="SUBSCRIBER_ID"  />
        </many-to-one>
		
	</class>
</hibernate-mapping>
