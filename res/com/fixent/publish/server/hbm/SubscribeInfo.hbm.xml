<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fixent.publish.server.model.SubscribeInfo" table="SUBSCRIBE_INFO">
		<id name="id" type="int">
			<column name="ID" precision="5" scale="0" />
			<generator class="assigned" />
		</id>
		
		<property name="noOfYear" type="int">
			<column name="NO_OF_YEAR" length="20" not-null="true" />
		</property>
		<property name="startDate" type="date">
			<column name="START_DATE" length="20" not-null="true" />
		</property>		
		<property name="endDate" type="date">
			<column name="END_DATE" not-null="true" />
		</property>
		
		<many-to-one name="book" class="com.fixent.publish.server.model.Book" column="BOOK_ID" not-null="true" />
		<many-to-one name="subscriber" class="com.fixent.publish.server.model.Subscriber" column="SUBSCRIBER_ID" not-null="true" />
		
	</class>
</hibernate-mapping>
